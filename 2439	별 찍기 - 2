#include <stdio.h>


int main(void)
{
	int num = 0;
	int a[100], b[100] = { 0, };
	scanf("%d", &num);
	for (int i = num; i > 0; i--)
	{
		for (int k = 0; k < i-1; k++) // i-1 를 안해서 띄어쓰기가 한번 더됫엇음 ...
		{
			printf(" ");
		}
		for (int j = num; j >= i; j--)
		{
			printf("*");
		}
		printf("\n");
	}
	return 0;
}
